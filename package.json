{
  "name": "book-operator-service",
  "version": "1.0.0",
  "description": "",
  "main": "index",
  "typings": "index",
  "scripts": {
    "start": "node ./dist/index.js",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage-unit",
    "build": "npm-run-all -s lint build:ts",
    "build:ts": "node ./node_modules/typescript/bin/tsc",
    "lint": "eslint ./src/index.ts src/*.ts src/**/*.ts",
    "lint-init": "eslint --init",
    "lint-fix": "eslint --fix src/**/*.ts",
    "test:unit": "jest --coverage --forceExit --detectOpenHandles --runInBand --config=jest.unit.config.js",
    "test-jenkins:unit": "jest --coverage --reporters=jest-junit --forceExit --detectOpenHandles --runInBand --config=jest.unit.config.js",
    "jest-clear": "jest --clearCache"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": ".",
    "outputName": "xunit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "request": "2.88.2",
    "body-parser": "1.20.2",
    "class-transformer": "0.5.1",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.14.0",
    "cloud-config-client": "1.6.2",
    "config": "3.3.9",
    "cookie-parser": "1.4.6",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "log4js": "6.9.1",
    "moment-timezone": "0.5.43",
    "node-health-service": "1.0.7",
    "pg": "^8.11.0",
    "request-context": "2.0.0",
    "routing-controllers": "0.10.4",
    "sprintf-js": "1.1.3",
    "swagger-ui-express": "5.0.0",
    "typedi": "0.8.0",
    "typeorm": "^0.3.17",
    "url-join": "4.0.1",
    "uuid": "9.0.1",
    "js-yaml": "4.1.0"
  },
  "devDependencies": {
    "@types/request": "2.48.5",
    "@types/body-parser": "1.19.4",
    "@types/config": "3.3.1",
    "@types/cookie-parser": "1.4.5",
    "@types/express": "4.17.20",
    "@types/helmet": "4.0.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^18.16.10",
    "@types/pg": "8.10.5",
    "@types/sprintf-js": "1.1.2",
    "@types/supertest": "2.0.14",
    "@types/swagger-ui-express": "4.1.4",
    "@types/url-join": "4.0.1",
    "@types/uuid": "9.0.5",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "babel-jest": "^29.7.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-n": "^16.2.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "16.0.0",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "typescript": "^4.9.5"
  }
}
